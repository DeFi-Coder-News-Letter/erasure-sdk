{
  "name": "@robinthomas/erasure-sdk",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "homepage": "https://github.com/robin-thomas/erasure-client#readme",
  "scripts": {
    "download": "./scripts/download.sh contracts",
    "prebuild": "npm run download",
    "build": "webpack --mode=production",
    "start": "webpack --watch",
    "pretest": "npm run download",
    "test": "NODE_ENV=test mocha --require esm --exit --bail -t 1200000 test/index.js",
    "coverage": "nyc npm run test",
    "doc:generate": "jsdoc src/* --destination erasure-docs -c jsdoc.json",
    "doc:deploy": "npm run doc:generate && now ./erasure-docs",
    "deploy": "npm run build && npm publish --access public"
  },
  "author": "Robin Thomas <robinthomas2591@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "3box": "^1.14.0",
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@erasure/crypto-ipfs": "1.0.0",
    "babel-loader": "^8.0.6",
    "bn.js": "^5.0.0",
    "bs58": "^4.0.1",
    "docdash": "^1.1.1",
    "esm": "^3.2.25",
    "ethers": "^4.0.40",
    "ganache-cli": "^6.7.0",
    "husky": "^3.0.5",
    "identity-wallet": "^1.0.0",
    "ipfs-http-client": "^39.0.2",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robin-thomas/erasure-client.git"
  },
  "bugs": {
    "url": "https://github.com/robin-thomas/erasure-client/issues"
  }
}
